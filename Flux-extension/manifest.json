{
  "manifest_version": 3,
  "name": "FetchX",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background/worker.js"
  },
  "permissions": [
    "downloads",
    "storage",
    "offscreen"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  }
}
